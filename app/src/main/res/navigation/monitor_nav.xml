<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/monitor_nav"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/monitorFragment">

    <fragment
        android:id="@+id/monitorFragment"
        android:name="com.example.privasee.ui.monitor.MonitorFragment"
        android:label="SetupOwnerFragment"
        tools:layout="@layout/fragment_monitor" >

        <action
            android:id="@+id/action_monitorFragment_to_monitorFragmentAppSnapshots"
            app:destination="@id/monitorFragmentAppSnapshots" />
        <action
            android:id="@+id/action_monitorFragment_to_monitorFragmenTimedSnapshots"
            app:destination="@id/monitorFragmenTimedSnapshots" />
    </fragment>

    <fragment
        android:id="@+id/monitorFragmentAppSnapshots"
        android:name="com.example.privasee.ui.monitor.MonitorFragmentAppSnapshots"
        android:label="SetupOwnerFragment"
        tools:layout="@layout/fragment_monitor_app_snapshots" >

        <action
            android:id="@+id/action_monitorFragmentAppSnapshots_to_monitorFragment"
            app:destination="@id/monitorFragment" />
    </fragment>


    <fragment
        android:id="@+id/monitorFragmenTimedSnapshots"
        android:name="com.example.privasee.ui.monitor.MonitorFragmentTimedSnapshots"
        android:label="SetupOwnerFragment"
        tools:layout="@layout/fragment_monitor_timed_snapshots" >

        <action
            android:id="@+id/action_monitorFragmenTimedSnapshots_to_monitorFragment"
            app:destination="@id/monitorFragment" />
    </fragment>

</navigation>